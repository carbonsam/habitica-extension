{
  "manifest_version": 2,

  "name": "Habitica",
  "description": "This  is the un-official Habitica browser extension",
  "version": "0.1",

  "browser_action": {
    "default_title": "Habitica",
    "default_icon": "icon.png",
    "default_popup": "dashboard.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}
